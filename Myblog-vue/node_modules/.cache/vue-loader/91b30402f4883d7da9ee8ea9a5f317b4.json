r.vue","sourceRoot":"src/components/common","sourcesContent":["<!--定义顶部导航栏-->\r\n<template >\r\n    <el-header class=\"me-area\">\r\n      <el-row class=\"me-header\" >\r\n<!--    logo 区域-->\r\n        <el-col :span=\"2\" class=\"me-header-left\" style=\"margin-left: 65px\">\r\n          <router-link to=\"/\"class=\"me-title\">\r\n            <img src=\"@/assets/img/logo.jpg\"/>\r\n          </router-link>\r\n        </el-col>\r\n<!--              if  -->\r\n        <el-col v-if=\"!simple\":span=\"16\">\r\n          <el-menu\r\n              :router=true\r\n              menu-trigger=\"click\"\r\n              active-text-color=\"#00aaff\"\r\n              :default-active=\"activeIndex\"\r\n              mode=\"horizontal\">\r\n            <el-menu-item index=\"/\">首页</el-menu-item>\r\n            <el-menu-item index=\"/category/all\">文章分类</el-menu-item>\r\n            <el-menu-item index=\"/tag/all\">标签</el-menu-item>\r\n            <el-menu-item index=\"/archives\">文章归档</el-menu-item>\r\n<!--            搜索文章-->\r\n            <el-menu-item >\r\n              <el-input size=\"mini\" style=\"margin-top:-8px;margin-left: 100px;width: 60% \" v-model=\"input\" placeholder=\"搜索文章\"></el-input>\r\n              <el-button size=\"small\" style=\"margin-top: -6px;margin-left: 2px;width: 15%;height: 50% \" class=\"el-icon-search\"></el-button>\r\n            </el-menu-item>\r\n            <el-col :span=\"3\" :offset=\"4\" style=\"margin-left: 110px\">\r\n              <el-menu-item index=\"/write\">\r\n                <i class=\"el-icon-edit\"></i>\r\n                写文章\r\n              </el-menu-item>\r\n            </el-col>\r\n          </el-menu>\r\n        </el-col>\r\n<!--              else  -->\r\n        <template v-else>\r\n          <slot></slot>\r\n        </template>\r\n\r\n        <el-col :span=\"4\">\r\n          <el-menu\r\n              :router=true\r\n              menu-trigger=\"click\"\r\n              mode=\"horizontal\"\r\n              active-text-color=\"#00aaff\">\r\n<!--              if  -->\r\n            <template v-if=\"!user.login\">\r\n              <el-menu-item index=\"/login\">\r\n                <el-button type=\"text\">登录</el-button>\r\n              </el-menu-item>\r\n              <el-menu-item index=\"/register\">\r\n                <el-button type=\"text\">注册</el-button>\r\n              </el-menu-item>\r\n            </template>\r\n<!--              else  -->\r\n            <template v-else>\r\n              <el-submenu index>\r\n                <template slot=\"title\">\r\n                  <img class=\"me-header-picture\" :src=\"user.avatar\"/>\r\n                </template>\r\n                <el-menu-item index @click=\"logout\"><i class=\"el-icon-back\"></i>退出</el-menu-item>\r\n              </el-submenu>\r\n            </template>\r\n          </el-menu>\r\n        </el-col>\r\n      </el-row>\r\n    </el-header>\r\n</template>\r\n\r\n<script>\r\nimport {Message} from \"element-ui\";\r\n\r\nexport default {\r\n  name: \"BaseHeader\",\r\n  props:{\r\n    activeIndex:String,\r\n    simple:{\r\n      type:Boolean,\r\n      default:false\r\n    }\r\n  },\r\n  data(){\r\n    return{\r\n    }\r\n  },\r\n  computed:{\r\n    //获取到登录用户 显示已登录\r\n    user(){\r\n      let login=this.$store.state.account.length!=0;\r\n      let avatar=this.$store.state.avatar;\r\n      return{\r\n        login,\r\n        avatar,\r\n      }\r\n    }\r\n  },\r\n  methods:{\r\n    logout(){\r\n          this.$store.dispatch(\"logout\").then((res)=>{\r\n            this.$router.push({path:'/'})\r\n          }).catch((error)=>{\r\n            if(error!=='error'){\r\n              this.$message({message:error,type:'error',showClose:true})\r\n            }\r\n          })\r\n      }\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n.el-button{\r\n  color: #00aaff;\r\n}\r\n.el-header {\r\n  position: fixed;\r\n  z-index: 1024;\r\n  min-width: 100%;\r\n  box-shadow: 0 2px 3px hsla(0, 0%, 7%, .1), 0 0 0 1px hsla(0, 0%, 7%, .1);\r\n}\r\n.me-title {\r\n  margin-top: 10px;\r\n  font-size: 24px;\r\n}\r\n.me-header-left {\r\n  margin-top: 10px;\r\n}\r\n.me-title img {\r\n  max-height: 2.4rem;\r\n  max-width: 100%;\r\n}\r\n\r\n.me-header-picture {\r\n  width: 36px;\r\n  height: 36px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 50%;\r\n  vertical-align: middle;\r\n  background-color: #00aaff;\r\n}\r\n</style>\r\n"]}]}