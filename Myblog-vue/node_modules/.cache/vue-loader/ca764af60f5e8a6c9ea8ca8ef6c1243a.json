nt=\"c\"-->\r\n<!--             :articleId=\"article.id\"-->\r\n<!--             :index=\"index\"-->\r\n<!--             :rootCommentCounts=\"comment.lenght\"-->\r\n<!--             @commentCountIncrement=\"commentCountIncrement\"-->\r\n<!--             :key=\"c.id\">-->\r\n<!--            </comment-item>-->\r\n          </div>\r\n        </div>\r\n      </el-main>\r\n    </el-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {findArticleById, getHotArticle} from '@/api/article'\r\nexport default {\r\n  name: \"BlogView\",\r\n  data(){\r\n    return{\r\n      article:{},\r\n      comment:{}\r\n    }\r\n  },\r\n  created() {\r\n    this.findArticleById();\r\n  },\r\n  methods:{\r\n    findArticleById(){\r\n      var id = his.$route.params.id;\r\n      //发起http请求，后端的数据\r\n      findArticleById().then((res)=>{\r\n        if(res.data.success){\r\n          this.article=res.data.data;\r\n        }else{\r\n          this.$message.error(res.data.msg);\r\n        }\r\n      }).catch((err)=>{\r\n        this.$message.error(\"系统错误\");\r\n      }).finally(()=>{\r\n      })\r\n    },\r\n    editArticle(){\r\n\r\n    },\r\n    tagOrCategory(){\r\n\r\n    },\r\n    publishComment(type,id){\r\n\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"]}]}