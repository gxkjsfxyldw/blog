n      userForm:{\r\n        account: '',\r\n        nickname: '',\r\n        password: ''\r\n      },\r\n      rules:{\r\n        account:[\r\n          {require:true,message:'请输入账号名称',trigger:'blur'},\r\n          {min:3,max:10,message:'长度在3到10个字符',trigger:'blur'}\r\n        ],\r\n        nickname:[\r\n          {require:true,message:'请输入昵称',trigger:'blur'},\r\n          {min:1,max:10,message:'长度在1到10个字符',trigger:'blur'}\r\n        ],\r\n        password:[\r\n          {require:true,message:'请输入密码',trigger:'blur'},\r\n          {min:3,max:15,message:'长度在3到15个字符',trigger:'blur'}\r\n        ],\r\n      }\r\n    }\r\n  },\r\n  methods:{\r\n    register(formName){\r\n      let that=this\r\n      this.$refs[formName].validate((valid)=>{\r\n        if(valid){\r\n          //注册的逻辑\r\n          //1.请求后端接口进行注册\r\n          //2.注册完成后跳转首页  返回一个token  令牌\r\n          //3.需要在前端存储token，此token存储用户信息，并且其他组件也可以取到\r\n          //涉及到状态流转\r\n          this.$store.dispatch(\"register\",this.userForm).then(()=>{\r\n            that.$message({message:'注册成功，快写文章吧',type:'success',showClose:true});\r\n            //注册成功后跳转到首页\r\n            that.$router.push({path:'/'})\r\n          }).catch((error)=>{\r\n            if(error!=='error'){\r\n              that.$message({message:error,type:'error',showClose:true});\r\n            }\r\n          })\r\n        }else{\r\n          return false;\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n#login {\r\n  min-width: 100%;\r\n  min-height: 100%;\r\n}\r\n\r\n.me-video-player {\r\n  background-color: transparent;\r\n  width: 100%;\r\n  height: 100%;\r\n  object-fit: fill;\r\n  display: block;\r\n  position: absolute;\r\n  left: 0;\r\n  z-index: 0;\r\n  top: 0;\r\n}\r\n\r\n.me-login-box {\r\n  position: absolute;\r\n  width: 300px;\r\n  height: 320px;\r\n  background-color: white;\r\n  margin-top: 150px;\r\n  margin-left: -180px;\r\n  left: 50%;\r\n  padding: 30px;\r\n}\r\n\r\n.me-login-box-radius {\r\n  border-radius: 10px;\r\n  box-shadow: 0px 0px 1px 1px rgba(161, 159, 159, 0.1);\r\n}\r\n\r\n.me-login-box h1 {\r\n  text-align: center;\r\n  font-size: 24px;\r\n  margin-bottom: 20px;\r\n  vertical-align: middle;\r\n}\r\n\r\n.me-login-design {\r\n  text-align: center;\r\n  font-family: 'Open Sans', sans-serif;\r\n  font-size: 18px;\r\n}\r\n\r\n.me-login-design-color {\r\n  color: #5FB878 !important;\r\n}\r\n\r\n.me-login-button {\r\n  text-align: center;\r\n}\r\n\r\n.me-login-button button {\r\n  width: 100%;\r\n}\r\n\r\n</style>\r\n\r\n"],"sourceRoot":"src/views"}]}