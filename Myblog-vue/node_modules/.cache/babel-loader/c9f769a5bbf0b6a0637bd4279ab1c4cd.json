rm\" :rules=\"rules\">\r\n        <el-form-item prop=\"account\">\r\n          <el-input placeholder=\"用户名\" v-model=\"userForm.account\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"password\">\r\n          <el-input placeholder=\"密码\" type=\"password\" v-model=\"userForm.password\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item size=\"small\" class=\"me-login-button\">\r\n          <el-button type=\"primary\" @click.n.native.prevent=\"login('userForm')\">登录</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Login\",\r\n  data(){\r\n    return{\r\n      userForm:{\r\n        account: '',\r\n        password: ''\r\n      },\r\n      rules:{\r\n        account:[\r\n          {require:true,message:'请输入账号名称',trigger:'blur'},\r\n          {min:3,max:10,message:'长度在3到10个字符',trigger:'blur'}\r\n        ],\r\n        password:[\r\n          {require:true,message:'请输入密码',trigger:'blur'},\r\n          {min:3,max:15,message:'长度在3到15个字符',trigger:'blur'}\r\n        ],\r\n      }\r\n    }\r\n  },\r\n  methods:{\r\n    login(formName){\r\n      let that=this;\r\n      this.$refs[formName].validate((valid)=>{\r\n        //判断表单是否合法\r\n        if(valid){\r\n          //将账号密码传入拦截器\r\n          that.$store.dispatch('login',that.userForm).then(()=>{\r\n            //如果成功就跳转回上一个界面\r\n            that.$router.go(-1)\r\n            that.$message({message:'登录成功，欢迎来到李达旺的个人博客',type:'success',showClose:true});\r\n          }).catch((error)=>{\r\n            if(error!=='error'){\r\n              that.$message({message:error,type:'error',showClose:true});\r\n            }\r\n          })\r\n        }else{\r\n          return false;\r\n        }\r\n      });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n#login {\r\n  min-width: 100%;\r\n  min-height: 100%;\r\n}\r\n\r\n.me-video-player {\r\n  background-color: transparent;\r\n  width: 100%;\r\n  